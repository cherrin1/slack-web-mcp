{
  "routes": [
    {
      "route": "/connect",
      "rewrite": "/api/index/connect"
    },
    {
      "route": "/register",
      "rewrite": "/api/index/register"
    },
    {
      "route": "/oauth/*",
      "rewrite": "/api/index/{path}"
    },
    {
      "route": "/*",
      "rewrite": "/api/index/{path}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/index"
  },
  "responseOverrides": {
    "401": {
      "statusCode": 401,
      "rewrite": "/api/index"
    }
  }
}
